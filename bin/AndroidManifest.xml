<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.mp3rc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.mp3rc.activitiys.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.mp3rc.activitiys.SettingsActivity"
            android:label="@string/pref_title_general" >
        </activity>

        <service android:name="de.mp3rc.playLogic.PlayService" >
        </service>

        <activity
            android:name="de.mp3rc.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="de.mp3rc.activitiys.RestoreActivity"
            android:label="@string/title_activity_restore" >
        </activity>
        <activity
            android:name="de.mp3rc.activitiys.DetailActivity"
            android:label="@string/title_activity_detail"
            android:theme="@style/mp3Rc_Dialog" >
        </activity>
        <activity
            android:name="de.mp3rc.activitiys.VisualizerActivity"
            android:label="@string/title_activity_visualizer" >
        </activity>
        <activity
            android:name="de.mp3rc.activitiys.EQActivity"
            android:label="@string/title_activity_eq" >
        </activity>
    </application>

</manifest>
